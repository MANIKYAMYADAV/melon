{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Open-Chef": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Open-Chef",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/css/app.min.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/icons.min.css",
              "src/assets/libs/ladda/ladda-themeless.min.css",
              "src/assets/libs/jquery-nice-select/nice-select.css",
              "src/assets/libs/switchery/switchery.min.css" ,
              "src/assets/libs/select2/select2.min.css",
              "src/assets/libs/bootstrap-select/bootstrap-select.min.css",
              "src/assets/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.css",
              "src/assets/libs/dropzone/dropzone.min.css",
              "src/assets/libs/cropper/cropper.min.css",
              "src/assets/libs/flatpickr/flatpickr.min.css",
              "src/assets/libs/bootstrap-colorpicker/bootstrap-colorpicker.min.css" ,
              "src/assets/libs/clockpicker/bootstrap-clockpicker.min.css",
              "src/assets/libs/quill/quill.core.css",
              "src/assets/libs/quill/quill.bubble.css",
              "src/assets/libs/quill/quill.snow.css",
              "src/assets/libs/bootstrap-table/bootstrap-table.min.css",
              "src/assets/libs/datatables/dataTables.bootstrap4.css",
              "src/assets/libs/datatables/responsive.bootstrap4.css",
              "src/assets/libs/datatables/buttons.bootstrap4.css",
              "src/assets/libs/datatables/select.bootstrap4.css",
              "src/assets/libs/jsgrid/jsgrid.min.css",
              "src/assets/libs/jsgrid/jsgrid-theme.css",
              "src/assets/libs/rwd-table/rwd-table.min.css",
              "src/assets/libs/tablesaw/tablesaw.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/vendor.min.js",
              "src/assets/js/app.min.js",
              "src/assets/libs/ladda/spin.js",
              "src/assets/libs/ladda/ladda.js",
              "src/assets/js/pages/loading-btn.init.js",
              "src/assets/libs/magnific-popup/jquery.magnific-popup.min.js",
              "src/assets/js/pages/gallery.init.js",
              "src/assets/libs/jquery-nice-select/jquery.nice-select.min.js",
              "src/assets/libs/switchery/switchery.min.js",
              "src/assets/libs/select2/select2.min.js",
              "src/assets/libs/jquery-mockjax/jquery.mockjax.min.js",
              "src/assets/libs/autocomplete/jquery.autocomplete.min.js",
              "src/assets/libs/bootstrap-select/bootstrap-select.min.js",
              "src/assets/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.min.js",
              "src/assets/libs/bootstrap-maxlength/bootstrap-maxlength.min.js",
              "src/assets/js/pages/form-advanced.init.js",
              "src/assets/libs/dropzone/dropzone.min.js",
              "src/assets/js/pages/form-fileuploads.init.js",
              "src/assets/libs/cropper/cropper.min.js",
              "src/assets/js/pages/form-imagecrop.init.js",
              "src/assets/libs/jquery-mask-plugin/jquery.mask.min.js",
              "src/assets/libs/autonumeric/autoNumeric-min.js",
              "src/assets/js/pages/form-masks.init.js",
              "src/assets/libs/flatpickr/flatpickr.min.js",
              "src/assets/libs/bootstrap-colorpicker/bootstrap-colorpicker.min.js",
              "src/assets/libs/clockpicker/bootstrap-clockpicker.min.js",
              "src/assets/js/pages/form-pickers.init.js",
              "src/assets/libs/katex/katex.min.js",
              "src/assets/libs/quill/quill.min.js",
              "src/assets/js/pages/form-quilljs.init.js",
              
              "src/assets/libs/parsleyjs/parsley.min.js",
              "src/assets/js/pages/form-validation.init.js",
              "src/assets/libs/twitter-bootstrap-wizard/jquery.bootstrap.wizard.min.js",
              "src/assets/js/pages/form-wizard.init.js",
              "src/assets/libs/moment/moment.min.js",
              "src/assets/libs/bootstrap-table/bootstrap-table.min.js",
              "src/assets/js/pages/bootstrap-tables.init.js",
              "src/assets/libs/datatables/jquery.dataTables.js",
              "src/assets/libs/datatables/dataTables.bootstrap4.js",
              "src/assets/libs/datatables/dataTables.responsive.min.js",
              "src/assets/libs/datatables/responsive.bootstrap4.min.js",
              "src/assets/libs/datatables/dataTables.buttons.min.js",
              "src/assets/libs/datatables/buttons.bootstrap4.min.js",
              "src/assets/libs/datatables/buttons.html5.min.js",
              "src/assets/libs/datatables/buttons.flash.min.js",
              "src/assets/libs/datatables/buttons.print.min.js",
              "src/assets/libs/datatables/dataTables.keyTable.min.js",
              "src/assets/libs/datatables/dataTables.select.min.js",
              "src/assets/libs/pdfmake/pdfmake.min.js",
              "src/assets/libs/pdfmake/vfs_fonts.js",
              "src/assets/js/pages/datatables.init.js",
              "src/assets/libs/jquery-tabledit/jquery.tabledit.min.js",
              "src/assets/js/pages/tabledit.init.js",
              "src/assets/js/pages/foo-tables.init.js",
              "src/assets/libs/jsgrid/jsgrid.min.js",
              "src/assets/js/pages/jsgrid.init.js",
              "src/assets/libs/rwd-table/rwd-table.min.js",
              "src/assets/js/pages/responsive-table.init.js",
              "src/assets/libs/tablesaw/tablesaw.js",
              "src/assets/js/pages/tablesaw.init.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Open-Chef:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Open-Chef:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Open-Chef:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Open-Chef:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Open-Chef:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "Open-Chef"
}
